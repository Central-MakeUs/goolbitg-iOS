apple_id(ENV.fetch("FASTLANE_APPLE_ID")) # Your Apple Developer Portal username

itc_team_id(ENV.fetch("FASTLANE_ITC_TEAM_ID")) # App Store Connect Team ID
team_id(ENV.fetch("FASTLANE_TEAM_ID")) # Developer Portal Team ID

